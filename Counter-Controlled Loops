#include <stdio.h>

int main()
{
    // Example 1: Print numbers 1 to 5
for (int i = 1; i <= 5; i++) {
    printf("%d\n", i);
}

// Example 2: Sum of first 10 natural numbers
int sum = 0;
for (int i = 1; i <= 10; i++) {
    sum += i;
}
printf("Sum = %d\n", sum);

// Example 3: Print even numbers from 2 to 10
for (int i = 2; i <= 10; i += 2) {
    printf("%d\n", i);
}

// Example 4: Countdown from 5 to 1
for (int i = 5; i >= 1; i--) {
    printf("%d\n", i);
}

// Example 5: Print squares of numbers 1 to 5
for (int i = 1; i <= 5; i++) {
    printf("%d squared = %d\n", i, i*i);
}


    return 0;
}