#include <stdio.h>

int main()
{
// Example 1: Print numbers 1 to 5
int i = 1;
while (i <= 5) {
    printf("%d\n", i);
    i++;
}

// Example 2: Sum of digits
int num = 1234, sum = 0;
while (num > 0) {
    sum += num % 10;
    num /= 10;
}
printf("Sum of digits: %d\n", sum);

// Example 3: Print even numbers less than 10
int j = 2;
while (j < 10) {
    printf("%d\n", j);
    j += 2;
}

// Example 4: Countdown from 5
int k = 5;
while (k > 0) {
    printf("%d\n", k);
    k--;
}

// Example 5: Print characters until 'z'
char ch = 'a';
while (ch <= 'z') {
    printf("%c\n", ch);
    ch++;
}

    return 0;
}
