#include <stdio.h>

int main() {
    int grade;
    
    printf("Enter your grade (0-100): ");
    scanf("%d", &grade);
    
    
    switch (grade / 10) {
        case 10:
        case 9:
            printf("Equivalent Grade: A\n");
            break;
        case 8:
            printf("Equivalent Grade: B\n");
            break;
        case 7:
        case 6:
            printf("Equivalent Grade: C\n");
            break;
        default:
            if (grade >= 0 && grade < 60) {
                printf("Equivalent Grade: Failed\n");
            } else {
                printf("Invalid grade entered.\n");
            }
            break;
    }

    return 0;
}
